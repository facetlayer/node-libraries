{
  "name": "@facetlayer/prism-framework-ui",
  "version": "0.2.2",
  "description": "UI component library for the Prism app framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p .",
    "dev": "tsc -p . --watch",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --testTimeout=30000"
  },
  "keywords": [
    "framework",
    "react",
    "nextjs",
    "typescript",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.525.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "^5.7.2",
    "vitest": "1.6.0"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "@tanstack/react-query-devtools": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
