{
  "name": "@facetlayer/prism-framework-api",
  "version": "0.2.3",
  "type": "module",
  "description": "API component of the Prism app framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "prism-api": "dist/cli.js"
  },
  "files": [
    "dist",
    "docs",
    "README.md"
  ],
  "scripts": {
    "build": "node build.mts build",
    "prepublishOnly": "node build.mts validate && node build.mts build",
    "typecheck": "tsc -p .",
    "dev": "tsc -p . --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "local-install": "pnpm build && npm i -g ."
  },
  "keywords": [
    "framework",
    "express",
    "electron",
    "typescript",
    "saas"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@facetlayer/doc-files-helper": "0.1.1",
    "@facetlayer/sqlite-wrapper": "^1.2.0",
    "@facetlayer/streams": "^1.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "openapi3-ts": "^4.5.0",
    "prom-client": "^15.1.3",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@facetlayer/build-config-nodejs": "^0.3.0",
    "@facetlayer/prism-framework-tools": "^0.2.3",
    "@facetlayer/subprocess-wrapper": "^1.1.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "^17.0.35",
    "esbuild": "^0.25.12",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
