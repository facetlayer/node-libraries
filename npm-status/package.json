{
  "name": "@facetlayer/npm-status",
  "version": "0.1.1",
  "description": "CLI tool to check NPM publish status and detect unpublished changes",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "npm-status": "dist/cli.js"
  },
  "scripts": {
    "build": "node build.mts build",
    "prepublishOnly": "node build.mts validate && node build.mts build",
    "typecheck": "tsc -p .",
    "test": "vitest run --testTimeout=30000",
    "local:install": "npm run build && npm i -g ."
  },
  "keywords": [
    "npm",
    "publish",
    "version",
    "cli"
  ],
  "author": "andyfischer",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "dependencies": {
    "@facetlayer/subprocess": "^1.1.1",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@facetlayer/build-config-nodejs": "0.3.1",
    "@types/node": "^22.10.2",
    "@types/yargs": "17.0.34",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0"
  }
}
